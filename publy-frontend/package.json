{
  "name": "publy-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "codegen": "graphql-codegen --config codegen.yml && prettier --write src/generated/**",
    "codegen:watch": "graphql-codegen --config codegen.yml --watch src/**/*.graphql",
    "prettier:check": "prettier --check src",
    "webserver": "rm -f out/404.html && serve -p 3030 --single out"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@heroicons/react": "^1.0.6",
    "@types/node": "20.1.0",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@types/remarkable": "^2.0.3",
    "autoprefixer": "10.4.14",
    "classnames": "^2.3.1",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.1",
    "graphql": "^16.5.0",
    "graphql-ws": "^5.10.0",
    "highlight.js": "^11.6.0",
    "next": "13.4.1",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.3.0",
    "remarkable": "^2.0.1",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.11.6",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-react-apollo": "^3.3.3",
    "serve": "^14.2.0"
  }
}
